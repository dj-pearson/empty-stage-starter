# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Sentry Configuration (Error Monitoring)
VITE_SENTRY_DSN=your_sentry_dsn
VITE_SENTRY_ENABLED=false
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# App Version
VITE_APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# Backup Scheduler (for cron jobs)
CRON_SECRET=your_cron_secret_key

# Email Configuration
EMAIL_PROVIDER=console # Options: console, resend, sendgrid
RESEND_API_KEY=your_resend_api_key
EMAIL_FROM=noreply@eatpal.com

# SEO API Keys
# PageSpeed Insights (FREE - Required for Core Web Vitals)
PAGESPEED_INSIGHTS_API_KEY=your_google_pagespeed_api_key

# Backlink APIs (Choose one - Optional)
AHREFS_API_KEY=your_ahrefs_api_key # $99/month - Most comprehensive
MOZ_ACCESS_ID=your_moz_access_id # $79/month
MOZ_SECRET_KEY=your_moz_secret_key

# SERP Tracking APIs (Choose one - Optional)
SERPAPI_KEY=your_serpapi_key # $50/month - Easiest to use
DATAFORSEO_LOGIN=your_dataforseo_login # $30/month - More affordable
DATAFORSEO_PASSWORD=your_dataforseo_password

# Google Search Console OAuth
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=https://your-domain.com/oauth/callback
