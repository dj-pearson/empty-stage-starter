# EatPal Environment Variables
# Copy this file to .env and fill in your values

# Required: Supabase Configuration
# Get these from your Supabase project dashboard or self-hosted instance
VITE_SUPABASE_URL=https://api.tryeatpal.com
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Optional: Supabase Edge Functions URL (if different from API URL)
VITE_FUNCTIONS_URL=https://functions.tryeatpal.com

# Optional: Error Monitoring with Sentry
VITE_SENTRY_DSN=
VITE_SENTRY_ENABLED=false

# Optional: Sentry Vite Plugin (for production sourcemaps)
SENTRY_ORG=
SENTRY_PROJECT=
SENTRY_AUTH_TOKEN=

# Optional: Email notifications
EMAIL_PROVIDER=resend
RESEND_API_KEY=

# Optional: SEO & Analytics APIs
PAGESPEED_INSIGHTS_API_KEY=

# Optional: Stripe (for payments)
VITE_STRIPE_PUBLISHABLE_KEY=

# Optional: Google Analytics
VITE_GA_MEASUREMENT_ID=
